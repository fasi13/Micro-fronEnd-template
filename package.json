{
  "name": "cxs.admin",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@testing-library/dom": "^7.31.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@types/testing-library__jest-dom": "^5.14.0",
    "axios": "^0.21.1",
    "mobx": "^6.3.2",
    "mobx-react": "^7.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.1.2"
  },
  "devDependencies": {
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.23",
    "@types/node": "^12.20.15",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "autoprefixer": "^10.2.6",
    "cypress": "^7.5.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.28.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sonarjs": "^0.8.0-125",
    "husky": "^6.0.0",
    "jest-junit": "^12.2.0",
    "lint-staged": "^11.0.0",
    "postcss": "^8.3.4",
    "prettier": "2.3.1",
    "pretty-quick": "^3.1.0",
    "tailwindcss": "^2.1.4",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "scripts": {
    "prepare": "husky install",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:dev": "env-cmd -e development npm run-script build",
    "build:staging": "env-cmd -e staging npm run-script build",
    "build:prod": "env-cmd -e production npm run-script build",
    "test": "jest",
    "test:coverage": "yarn test --coverage --watchAll=false",
    "lint": "eslint ./ --ignore-path .gitignore",
    "lint:fix": "eslint ./ --fix",
    "format": "prettier --write \"**/*.{ts,js,jsx,tsx}\"",
    "test:ci": "jest --watchAll=false --coverage --reporters=default --reporters=jest-junit",
    "cypress:open": "cypress open",
    "eject": "react-scripts eject"
  },
  "lint-staged": {
    "*.+(ts|tsx|js|jsx)": [
      "npm run format",
      "npm run lint:fix"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest-junit": {
    "suiteNameTemplate": "{filepath}",
    "outputDirectory": ".",
    "outputName": "test-results.xml"
  }
}
